<p id="notice"><%= notice %></p>

<h1>Listing B Corpus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>End date build</th>
      <th>B queues</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_corpus.each do |b_corpu| %>
      <tr>
        <td><%= b_corpu.name %></td>
        <td><%= b_corpu.description %></td>
        <td><%= b_corpu.end_date_build %></td>
        <td><%= b_corpu.b_queues %></td>
        <td><%= link_to 'Show', b_corpu %></td>
        <td><%= link_to 'Edit', edit_b_corpu_path(b_corpu) %></td>
        <td><%= link_to 'Destroy', b_corpu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

